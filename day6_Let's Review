# Enter your code here. Read input from STDIN. Print output to STDOUT
def orderd(word):
    even=[]
    odd=[]
    for i in range(0,len(word),2):
        even.append(word[i])
    for j in range(1,len(word),2):
        odd.append(word[j])
    print("".join(even),"".join(odd))

if __name__ == "__main__":
    n=int(input())
    for i in range(n):
        s = input()
        orderd(s)
